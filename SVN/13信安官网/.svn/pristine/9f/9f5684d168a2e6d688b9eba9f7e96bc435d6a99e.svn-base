#logo
{
	overflow:hidden;
	width:300px;
	height:150px;
	background:#EEE;
}
span
{
	opacity:0;
	display:block;
	height:50px;
	font:32px tahoma,'Hiragino Sans GB','Wenquanyi Micro Hei','\5FAE\8F6F\96C5\9ED1',Arial,Verdana,sans-serif;
}
*{
	text-shadow:2px 2px 5px #6B6B6B;
}
.a1{
	-webkit-transform:translate(60px);
	-webkit-animation:animations 2s ease-out alternate;
	-moz-transform:translate(55px) ;
	-moz-animation:animations 2s ease-out alternate;
	-o-transform:translate(55px) ;
	-o-animation:animations 2s ease-out alternate;
	-ms-transform:translate(55px) ;
	-ms-animation:animations 2s ease-out alternate;
	transform:translate(55px) ;
	animation:animations 2s ease-out alternate;
}
@-webkit-keyframes animations{
	0%{-webkit-transform:translate(0);opacity:0;}
	50%{-webkit-transform:translate(30px);opacity:1;}
	70%{-webkit-transform:translate(35px);opacity:1;}
	100%{-webkit-transform:translate(60px);opacity:0;}
}
@-moz-keyframes animations{
	0%{-moz-transform:translate(0);opacity:0;}
	50%{-moz-transform:translate(30px);opacity:1;}
	70%{-moz-transform:translate(35px);opacity:1;}
	100%{-moz-transform:translate(60px);opacity:0;}
}
@-o-keyframes animations{
	0%{-o-transform:translate(0);opacity:0;}
	50%{-o-transform:translate(30px);opacity:1;}
	70%{-o-transform:translate(35px);opacity:1;}
	100%{-o-transform:translate(60px);opacity:0;}
}
@-ms-keyframes animations{
	0%{-ms-transform:translate(0);opacity:0;}
	50%{-ms-transform:translate(30px);opacity:1;}
	70%{-ms-transform:translate(35px);opacity:1;}
	100%{-ms-transform:translate(60px);opacity:0;}
}
@keyframes animations{
	0%{transform:translate(0);opacity:0;}
	50%{transform:translate(30px);opacity:1;}
	70%{transform:translate(35px);opacity:1;}
	100%{transform:translate(60px);opacity:0;}
}
.a3{
	-webkit-transform:translate(80px) ;
	-webkit-animation:animations3 2s ease-out 2s ;
	-moz-transform:translate(80px );
	-moz-animation:animations3 2s ease-out 2s ;
	-o-transform:translate(80px) ;
	-o-animation:animations3 2s ease-out 2s ;
	-ms-transform:translate(80px) ;
	-ms-animation:animations3 2s ease-out 2s ;
	transform:translate(80px) ;
	animation:animations3 2s ease-out 2s ;
}
@-webkit-keyframes animations3{
	0%{-webkit-transform:translate(140px);opacity:0;}
	50%{-webkit-transform:translate(110px);opacity:1;}
	70%{-webkit-transform:translate(105px);opacity:1;}
	100%{-webkit-transform:translate(80px);opacity:0;}
}
@-moz-keyframes animations3{
	0%{-moz-transform:translate(140px);opacity:0;}
	50%{-moz-transform:translate(110px);opacity:1;}
	70%{-moz-transform:translate(105px);opacity:1;}
	100%{-moz-transform:translate(80px);opacity:0;}
}
@-o-keyframes animations3{
	0%{-o-transform:translate(140px);opacity:0;}
	50%{-o-transform:translate(110px);opacity:1;}
	70%{-o-transform:translate(105px);opacity:1;}
	100%{-o-transform:translate(80px);opacity:0;}
}
@-ms-keyframes animations3{
	0%{-ms-transform:translate(140px);opacity:0;}
	50%{-ms-transform:translate(110px);opacity:1;}
	70%{-ms-transform:translate(105px);opacity:1;}
	100%{-ms-transform:translate(80px);opacity:0;}
}
@keyframes animations3{
	0%{transform:translate(140px);opacity:0;}
	50%{transform:translate(110px);opacity:1;}
	70%{transform:translate(105px);opacity:1;}
	100%{transform:translate(80px);opacity:0;}
}
.a2{
	text-align:center;font-size:40px;
	-webkit-animation:animations2 5s ease-in-out 4s forwards;
	-moz-animation:animations2 5s ease-in-out 4s forwards;
	-o-animation:animations2 5s ease-in-out 4s forwards;
	-ms-animation:animations2 5s ease-in-out 4s forwards;
	animation:animations2 5s ease-in-out 4s forwards;
}
@-webkit-keyframes animations2{
	0%{opacity:0;}
	40%{opacity:.8;}
	45%{opacity:.3;}
	50%{opacity:.8;}
	55%{opacity:.3;}
	60%{opacity:.8;}
	100%{opacity:1;}
}
@-moz-keyframes animations2{
	0%{opacity:0;}
	40%{opacity:.8;}
	45%{opacity:.3;}
	50%{opacity:.8;}
	55%{opacity:.3;}
	60%{opacity:.8;}
	100%{opacity:1;}
}
@-o-keyframes animations2{
	0%{opacity:0;}
	40%{opacity:.8;}
	45%{opacity:.3;}
	50%{opacity:.8;}
	55%{opacity:.3;}
	60%{opacity:.8;}
	100%{opacity:1;}
}
@-ms-keyframes animations2{
	0%{opacity:0;}
	40%{opacity:.8;}
	45%{opacity:.3;}
	50%{opacity:.8;}
	55%{opacity:.3;}
	60%{opacity:.8;}
	100%{opacity:1;}
}
@keyframes animations2{
	0%{opacity:0;}
	40%{opacity:.8;}
	45%{opacity:.3;}
	50%{opacity:.8;}
	55%{opacity:.3;}
	60%{opacity:.8;}
	100%{opacity:1;}
}